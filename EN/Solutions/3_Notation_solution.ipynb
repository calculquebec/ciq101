   "source": [
    "import numpy as np\n",
    "\n",
    "# Define the vectors of the 0 and 1 states\n",
    "u = [1,0]\n",
    "v = [0,1]\n",
    "\n",
    "# Scalar product (numpy.dot)\n",
    "prods_uv = np.dot(u,v)\n",
    "print(prods_uv)"
   ]
